	function ViewADHide()
	{
		document.getElementById("adviewiframe").src = '';
		$("#adviewiframe").removeAttr("width");
		$("#adviewiframe").removeAttr("height");
		$("#adviewiframe").removeAttr("style");
		$("#eMeng").hide();
	}
